{
  "swagger": "2.0",
  "info": {
    "version": "v1",
    "title": "DynamicSwagger"
  },
  "paths": { {{#each Entities}}
    "/{{Name}}": {
      "get": {
        "tags": [ "{{Name}}" ],
        "operationId": "{{Name}}Get",
        "consumes": [],
        "produces": [ "application/json" ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "Success",
            "schema": { "$ref": "#/definitions/ResultApiIEnumerable{{Name}}" }
          }
        }
      },
      "post": {
        "tags": [ "{{Name}}" ],
        "operationId": "{{Name}}Post",
        "consumes": [ "application/json"],
        "produces": [ "application/json" ],
        "parameters": [
          {
            "name": "item",
            "in": "body",
            "required": false,
            "schema": { "$ref": "#/definitions/Model{{Name}}" }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": { "$ref": "#/definitions/ResultApiBoolean" }
          }
        }
      }
    },
    "/{{Name}}/{id}": {
      "get": {
        "tags": [ "{{Name}}" ],
        "operationId": "{{Name}}ByIdGet",
        "consumes": [],
        "produces": [ "application/json" ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": { "$ref": "#/definitions/ResultApi{{Name}}" }
          }
        }
      },
      "put": {
        "tags": [ "{{Name}}" ],
        "operationId": "{{Name}}ByIdPut",
        "consumes": [],
        "produces": [ "application/json" ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "item",
            "in": "body",
            "required": false,
            "schema": { "$ref": "#/definitions/Model{{Name}}" }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": { "$ref": "#/definitions/ResultApiBoolean" }
          }
        }
      },
      "delete": {
        "tags": [ "{{Name}}" ],
        "operationId": "{{Name}}ByIdDelete",
        "consumes": [],
        "produces": [ "application/json" ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": { "$ref": "#/definitions/ResultApiBoolean" }
          }
        }
      }
    }{{#if HasMore}}, {{/if}}{{/each}}
  },
  "definitions": {
    "ResultApiBoolean": {
        "type": "object",
        "properties": {
        "message": { "type": "string" },
        "result": { "type": "boolean" }
        }
    }{{#each Entities}},
    "Model{{Name}}": {
        "type": "object",
        "properties": {
        "id": {
            "format": "int64",
            "type": "integer"
        },
{{#each Attributes}}
    "{{Name}}": { "format": "{{TypeLanguage.Format}}",
                  "type": "{{TypeLanguage.Type}}" }{{#if HasMore}}, {{/if}}
{{/each}}
        }
    },
    "ResultApiIEnumerable{{Name}}": {
        "type": "object",
        "properties": {
        "message": { "type": "string" },
        "result": {
            "type": "array",
            "items": { "$ref": "#/definitions/Model{{Name}}" }
        }
        }
    },
    "ResultApi{{Name}}": {
        "type": "object",
        "properties": {
        "message": { "type": "string" },
        "result": { "$ref": "#/definitions/Model{{Name}}" }
        }
    }{{/each}}
  }
}
